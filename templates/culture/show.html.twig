{% extends 'base.html.twig' %}

{% block title %}Culture{% endblock %}

{% block body %}
    <h1>Culture</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ culture.id }}</td>
            </tr>
            <tr>
                <th>Year</th>
                <td>{{ culture.year ? culture.year|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>DateSpray</th>
                <td>{{ culture.dateSpray ? culture.dateSpray|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Period</th>
                <td>{{ culture.period ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('culture_index') }}">back to list</a>

    <a href="{{ path('culture_edit', {'id': culture.id}) }}">edit</a>

    {{ include('culture/_delete_form.html.twig') }}
{% endblock %}
